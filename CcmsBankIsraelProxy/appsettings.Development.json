{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "CcmsBankIsraelProxy": "Trace"
    }
  },
  "NLog": {
    "throwConfigExceptions": true,
    "targets": {
      "logconsole": {
        "type": "Console"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Debug",
        "writeTo": "logconsole"
      }
    ]
  },
  "Boi": {
    "Sap": {
      "Enabled": false
    },
    "Srhr": {
      "Enabled": false
    }
  }
}
